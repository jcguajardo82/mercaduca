    function whatKey(evt) {
        var eventReference = (typeof evt !== "undefined") ? evt : event;
        var keyCode = eventReference.keyCode;
        if (keyCode === 13) {
            alert("La tecla Enter ha sido presionada.");
            // Código para validar el formulario y enviarlo.
        }
        else if (keyCode === 9) {
